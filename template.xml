          <li>
            <a href=''><i class='fa fa-angle-double-right' aria-hidden='true'></i></a>
          </li>
        </ul>
      </nav>
    </b:includable>
    <!-- main section - index -->
    <!-- main section - archive -->
    <b:includable id='archive' var='top'>
      <h1>archive</h1>
    </b:includable>
    <!-- main section - archive -->
    <!-- main section - item -->
    <b:includable id='item' var='top'>
      <h1>item</h1>
    </b:includable>
    <!-- main section - item -->
    <!-- main section - static -->
    <b:includable id='static' var='top'>
      <h1>static_page</h1>
    </b:includable>
    <!-- main section - static -->
    <!-- main section - index-post -->
    <b:includable id='index-post' var='post'>
      <div class='index-post-item hentry'>
        <h2 class='index-post-title entry-title'>
          <data:post.title />
        </h2>
        <div class='index-post-header'>
        </div>
        <div class='index-post-body'>
          <div expr:id='"summary" + data:post.id' style='display:none;'>
