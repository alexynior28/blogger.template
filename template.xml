  margin-top: 0;
  margin-bottom: 0;
  padding: 8px 15px;
  line-height: 34px;
}

.navbar-brand h1 {
  margin-top: 0;
  margin-bottom: 0;
  font-size: 34px;
}

.navbar-default {
  background-color: #333333;
  border-color: transparent;
}

.navbar-default a.navbar-brand,
.navbar-default .navbar-nav>li>a {
  color: #dddddd;
  text-decoration: none;
}

.navbar-default a.navbar-brand:hover,
.navbar-default .navbar-nav>li>a:hover {
