/* xs size */
@media (max-width: 767px) {
  .navbar-menu {
    display: none;
  }

  .navbar-menu.in {
    display: block;
  }

  .header-btn-toggle-menu {
    margin-right: 15px;
    float: right;
  }
}

/* sm size only*/
@media (min-width: 768px) and (max-width: 991px) {
  .navbar-container {
    position: relative;
  }

  .navbar-form {
    position: absolute;
    top: 0;
    left: 6em;
    right: 0;
    margin-left: 15px;
    padding: 0;
    display: none;
  }

  .navbar-form.in {
    display: block;
  }

  .form-control {
    width: 100%;
  }

  .navbar-form-group {
    width: 100%;
    padding-left: 15px;
    padding-right: 15px;
  }

  .navbar-form-group .input-group {
    width: 100%;
  }

  .navbar-form-group .input-group-btn {
    width: 38px !important;
  }

  .header-btn-toggle-search{
    margin-right: 0;
  }
}

