}


/*---------------------------------------------------
    トップページ (index-xxx)
---------------------------------------------------*/
/* Blogger style overwrite */
.jumbotron {
  color: #efefef;
  position: relative;
  background: transparent;
}

.jumbotron .bg-container {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: -1;
}

.jumbotron .bg-image {
  background-image: url("//2.bp.blogspot.com/-a0sVOUkRCE4/WGXPo2UvkXI/AAAAAAAADt8/6m3lwX7KQ5g5Gi2t0VJpwhExkFeRkOHbQCLcB/s1600/jumbotron-wood.jpg");
  background-size: 100% auto;
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
}

.jumbotron .bg-cover {
  background-color: #000;
  opacity: 0.4;
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
}

.index-post-item {
  background: #fff;
  margin: 0 0 20px;
  position: relative;
  border: 1px solid #ddd;
  border-radius: 4px;
}

.index-post-item a {
  color: #222;
  text-decoration: none;
  display: inline-block;
  width: 100%;
}

.index-post-item a:hover {
  color: #444;
  background-color: #efefef;
  border-color: #adadad;
}

.index-post-title {
  margin: 0;
  padding: 10px 20px;
}

.index-post-header {
  margin: 10px 20px;
}

.index-post-body {
  margin: 0px 20px;
  min-height: 120px;
}

.index-post-thumbnail {
  float: left;
  margin-right: 10px;
  width: 150px;
  height: 120px;
  background: center 0 no-repeat transparent;
  background-size: 150px auto;
}

.index-post-footer {
  clear: both;
  color: #888;
  font-size: 12px;
  padding: 10px 20px;
}

.post-author, .post-timestamp {
  margin-right: 1em;
}

