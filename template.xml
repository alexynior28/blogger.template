      <section class='col-xs-12 col-sm-12 col-lg-8'>
        <div class='container-fluid'>
<b:section class='main' id='main' preferred='yes' showaddelement='no'>
  <b:widget id='Blog1' locked='true' title='Blog Posts' type='Blog'>
    <!-- main section - main -->
    <b:includable id='main' var='top'>
      <b:if cond='data:blog.pageType == "index"'>
        <b:include name='index' var='top' />
      <b:elseif cond='data:blog.pageType == "archive"' />
        <b:include name='archive' var='top' />
      <b:elseif cond='data:blog.pageType == "item"' />
        <b:include name='item' var='top' />
      <b:elseif cond='data:blog.pageType == "static_page"' />
        <b:include name='static_page' var='top' />
      <b:else />
        <h1>ERROR</h1>
      </b:if>
    </b:includable>
    <!-- main section - main -->
    <!-- main section - index -->
    <b:includable id='index' var='top'>
      <h1>index</h1>
      <div class='index-post-list'>
        <b:loop var='post' values='data:posts'>
          <b:include name='index-post' data='post' />
        </b:loop>
      </div>
      <nav class='text-center'>
        <ul class='pagination'>
          <li class='disabled'>
            <a href=''><i class='fa fa-angle-double-left' aria-hidden='true'></i></a>
